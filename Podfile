platform :ios, '10.0'
source 'https://github.com/CocoaPods/Specs.git'

def randomPods
  # Image managers
  pod 'Kingfisher' # Downloading and caching images
  # UI Elements
  pod 'NVActivityIndicatorView' # Loading view
end

target 'randomUser' do
    randomPods
end

target 'randomUserTests' do
    randomPods
end

target 'randomUserUITests' do
    randomPods
end
